<table class="table">
    <tr>
        <th>Nombre</th>
        <th>Descripcion</th>
        <th>Fecha de Inicio</th>
        <th>Fecha de Fin</th>
        <th>Estado</th>
    </tr>
    <% @projects.each do |p| %>
        <tr>
            <td><%= p.name %></td>
            <td><%= p.description %></td>
            <td><%= p.date_start %></td>
            <td><%= p.date_end %></td>
            <td><%= p.state %></td>
        </tr>
    <% end %>
</table>
<form>
        <label for="state">Estado</label>
        <select name="state">
            <option value="propuesta">propuesta</option>
            <option value="progreso">progreso</option>
            <option value="terminado">terminado</option>
        </select>
        <button type="submit" class="btn btn-primary">Buscar</button>
</form>




<%= link_to 'Volver al Inicio', root_path %><br>